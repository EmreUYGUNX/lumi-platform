# Lumi Commerce production environment template
NODE_ENV=production
APP_NAME=Lumi Commerce Platform
APP_PORT=8080
PORT=8080
API_BASE_URL=https://api.lumi-commerce.com
FRONTEND_URL=https://app.lumi-commerce.com
DATABASE_URL=postgresql://USER:PASSWORD@HOST:5432/DB_NAME
REDIS_URL=redis://USER:PASSWORD@HOST:6379
STORAGE_BUCKET=lumi-production-assets
LOG_LEVEL=warn
JWT_SECRET=replace-with-production-secret-value!!
SENTRY_DSN=https://public@sentry.io/1
FEATURE_FLAGS={"betaCheckout":false,"enableDebugToolbar":false}
CONFIG_HOT_RELOAD=false
CONFIG_ENCRYPTION_KEY=provide-32-byte-key
CI=true
