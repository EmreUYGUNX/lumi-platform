# Lumi Commerce example environment with dummy values
NODE_ENV=development
APP_NAME=Lumi Commerce (Example)
APP_PORT=4001
PORT=4001
API_BASE_URL=http://localhost:4001
FRONTEND_URL=http://localhost:3001
DATABASE_URL=postgresql://example:example@localhost:5555/example
REDIS_URL=redis://localhost:6380
STORAGE_BUCKET=lumi-example
LOG_LEVEL=info
JWT_SECRET=example-secret-placeholder-32chars!!
JWT_ACCESS_SECRET=example-access-secret-placeholder-32chars!!
JWT_REFRESH_SECRET=example-refresh-secret-placeholder-32chars!!
JWT_ACCESS_TTL=15m
JWT_REFRESH_TTL=14d
COOKIE_DOMAIN=example.local
COOKIE_SECRET=example-cookie-secret-placeholder-32chars!!
SENTRY_DSN=https://public@sentry.example.com/1
FEATURE_FLAGS={"betaCheckout":true,"enableDebugToolbar":false}
CONFIG_HOT_RELOAD=false
CONFIG_ENCRYPTION_KEY=example-encryption-key-change
CI=false
EMAIL_VERIFICATION_TTL=24h
PASSWORD_RESET_TTL=1h
SESSION_FINGERPRINT_SECRET=example-fingerprint-secret-placeholder-32chars!!
LOCKOUT_DURATION=15m
MAX_LOGIN_ATTEMPTS=5
