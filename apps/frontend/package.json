{
  "name": "@lumi/frontend",
  "version": "0.0.0",
  "private": true,
  "description": "Future Next.js web client for Lumi commerce platform.",
  "scripts": {
    "dev": "node -r ./scripts/polyfill-self.cjs ../../node_modules/.bin/next dev",
    "build": "rimraf .next-build && node -r ./scripts/polyfill-self.cjs ../../node_modules/.bin/next build",
    "start": "node -r ./scripts/polyfill-self.cjs ../../node_modules/.bin/next start",
    "lint": "NODE_ENV=development eslint --cache --cache-location .eslintcache \"src/**/*.{ts,tsx}\" \"middleware.ts\"",
    "typecheck": "tsc --noEmit",
    "format": "prettier \"src/**/*.{ts,tsx}\" --check",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "analyze": "ANALYZE=true node -r ./scripts/polyfill-self.cjs ../../node_modules/.bin/next build",
    "sitemap": "next-sitemap",
    "postbuild": "pnpm exec next-sitemap && node ./scripts/fix-standalone.js && node ./scripts/fix-routes-manifest.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@lumi/shared": "workspace:*",
    "@lumi/types": "workspace:*",
    "@lumi/ui": "workspace:*",
    "@next/bundle-analyzer": "^15.5.4",
    "@next/third-parties": "^16.0.4",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sentry/nextjs": "^8.55.0",
    "@tanstack/react-query": "^5.90.9",
    "@tanstack/react-query-devtools": "^5.90.2",
    "autoprefixer": "^10.4.22",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "critters": "^0.0.25",
    "framer-motion": "^12.23.24",
    "gsap": "^3.13.0",
    "immer": "^10.1.1",
    "lucide-react": "^0.554.0",
    "next": "^15.5.9",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.66.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "yet-another-react-lightbox": "^3.25.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@lumi/testing": "workspace:*",
    "@playwright/test": "^1.49.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitest/coverage-istanbul": "^2.1.9",
    "@vitest/coverage-v8": "^2.1.9",
    "axe-core": "^4.10.0",
    "identity-obj-proxy": "^3.0.0",
    "msw": "^2.4.9",
    "next-sitemap": "^4.2.3",
    "tailwindcss": "^3.4.18",
    "vitest": "^2.1.9",
    "webpack": "^5.103.0"
  }
}
